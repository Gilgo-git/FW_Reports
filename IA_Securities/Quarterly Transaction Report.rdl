  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DateParameter">
            <Value>=Parameters!DateParameter.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
declare @QTR_START datetime,@QTR_END datetime,
 @QTR_START2 datetime,@QTR_END2 datetime


SEt @QTR_END =DATEADD (dd, -1, DATEADD(qq, DATEDIFF(qq, 0,
CAST(@DateParameter AS datetime)

+1) , 0))
set @QTR_START=DATEADD(q, DATEDIFF(q, 0, @QTR_END), 0)
SEt @QTR_START2 =  DATEADD(qq, DATEDIFF(qq, 0, @QTR_END) - 1, 0)
SEt @QTR_END2 =  DATEADD (dd, -1, DATEADD(qq, DATEDIFF(qq, 0,  @QTR_END) +0, 0))
 
select a.commissionType,a.productCode,
sum(a.count6) as count6,
sum(a.count3) as count3,
@QTR_START2 as QTR_START2 ,
@QTR_END2 as QTR_END2 ,
@QTR_START as QTR_START ,
@QTR_END as QTR_END 
from (
select case when r.managedInd = -1 then 'MANAGED' 
	when r.fixedFeeInd = -1 then 'FEE BASED'
	else ''
	end as  commissionType,

	case when c.SECURITY_GROUP_CODE in ('RW','OPT','COM') then 'EQT'
	when c.SECURITY_GROUP_CODE in ('GIC', 'SBD') then  'FIX'
	else  c.SECURITY_GROUP_CODE end  as productCode,
	c.PROCESS_DATE,
	DATEADD(month,-6,getdate()) as M6,
	DATEADD(month,-3,getdate()) as M3,
	--datepart(quarter,c.PROCESS_DATE),
	case when c.PROCESS_DATE &gt;= @QTR_START2 and c.PROCESS_DATE &lt; @QTR_END2 + 1 then 1 else 0 end as count3,
	case when c.PROCESS_DATE &gt;= @QTR_START and c.PROCESS_DATE &lt;  @QTR_END + 1 then 1 else 0 end as count6
 
from ias_portal.BACKOFFICE.CMSDT c
inner join ias_portal.IAVM.rep r on
r.repId = c.RR_NUMBER
where
	c.TRANSACTION_TYPE = 'TRD' and
	c.TRANSACTION_GROUP_CODE != 'SWT' and
	--c.PROCESS_DATE between DATE_SUB(curdate(), INTERVAL 9 MONTH) and curdate()
	c.PROCESS_DATE &gt;= @QTR_START2 and c.PROCESS_DATE &lt; @QTR_END + 1
 
) a
 
group by a.commissionType,a.productCode
order by a.commissionType,a.productCode
 </CommandText>
