  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>iAS_Portal</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Date">
            <Value>=Parameters!Date.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--DECLARE @Date as date = '2021-05-03'

SELECT 
	[IAVMACT].A_C_Representative
	,rep.repName
	,case when [IAVMACT].ASC_3_Managed_Type in ('F','S','R') THEN 'Summit'
	when [IAVMACT].ASC_3_Managed_Type in ('L','X') THEN 'Diversiflex'
	when [IAVMACT].ASC_3_Managed_Type in ('M','Q') THEN 'Managed'
	when [IAVMACT].ASC_3_Managed_Type in ('V','N') THEN 'Vintage'
	when [IAVMACT].ASC_3_Managed_Type ='J' THEN 'Strategic Portfolio'
	END AS [Platform]
	,COUNT(DISTINCT IAVMACT.A_C_ID) AS Accounts
	,SUM(position.valueCAD) as AUA
FROM [BACKOFFICE].[IAVMACT] (nolock)
	LEFT OUTER JOIN [IAVM].[rep] (nolock)
		ON [IAVMACT].A_C_Representative = rep.repId
	INNER JOIN [IAVM].[position] (nolock)
		ON [IAVMACT].A_C_ID = position.accountId
WHERE position.processDate = @Date
and ([IAVMACT].A_C_Closing_Date &gt;= @Date OR NULLIF([IAVMACT].A_C_Closing_Date,'') IS NULL)
AND [IAVMACT].ASC_3_Managed_Type in ('V','N','J','M','Q','L','X','F','S','R') 
GROUP BY [IAVMACT].A_C_Representative
	,rep.repName
	,case when [IAVMACT].ASC_3_Managed_Type in ('F','S','R') THEN 'Summit'
	when [IAVMACT].ASC_3_Managed_Type in ('L','X') THEN 'Diversiflex'
	when [IAVMACT].ASC_3_Managed_Type in ('M','Q') THEN 'Managed'
	when [IAVMACT].ASC_3_Managed_Type in ('V','N') THEN 'Vintage'
	when [IAVMACT].ASC_3_Managed_Type ='J' THEN 'Strategic Portfolio'
	END</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
