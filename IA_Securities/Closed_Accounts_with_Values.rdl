<?xml version="1.0" encoding="utf-8"?>
<Report MustUnderstand="df" xmlns="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns:df="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition/defaultfontfamily">
  <df:DefaultFontFamily>Segoe UI</df:DefaultFontFamily>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="iAS_Portal">
      <DataSourceReference>iAS_Portal</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>b063ea80-99a6-49ab-95a3-6e0300639490</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>iAS_Portal</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@RptDate">
            <Value>=Parameters!RptDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
    c.A_C_Representative,
    ISNULL(r.repName, '') AS repName,
    ISNULL(r.branchCode, '') AS branchCode,
    c.A_C_Closing_Date,
    c.A_C_Closure_Reason Reason_Code,
    CASE c.A_C_Closure_Reason
        WHEN '1' THEN 'External Transfer'     
        WHEN '2' THEN 'Internal Transfer'     
        WHEN '3' THEN 'Full Dereg'     
        WHEN '5' THEN 'To Purchase Annuity'     
        WHEN '6' THEN 'Estate'     
        WHEN '7' THEN 'Bankruptcy'     
        WHEN '9' THEN 'Opened in Error'
        WHEN ''  THEN ''
        ELSE 'UNDEFINED'
    END Reason_Description,
    c.A_C_ID,
    c.A_C_Short_Name,
    c.A_C_Language,
    c.Client_Residence_Region,
    -1 * v.A_C_Summ_Equity AS A_C_Summ_Equity,
    v.A_C_Summ_Mkt_Value AS A_C_Summ_Mkt_Value,
    p.TI_Symbol AS symbol
FROM
    BACKOFFICE.IAVMACT c
        LEFT JOIN IAVM.rep r
            ON c.A_C_Representative = r.repId
        LEFT JOIN BACKOFFICE.IAVMVAL v
            ON v.A_C_ID = c.A_C_ID
                AND v.A_C_Summ_Balance_Bus_Date = 
					(
                        SELECT 
                            Last_Maint_Date
                        FROM
                            BACKOFFICE.IAVMSM
                        WHERE
                            ti_alternate_id = '000009'
					)
        LEFT JOIN  ( --BACKOFFICE.IAVMPSN 
		                SELECT t2.A_C_ID,t2.Tran_Summ_Business_Date,
                                    STUFF(
                                            ( SELECT ',' + CAST(TI_Symbol AS varchar) 
		                                        FROM BACKOFFICE.IAVMPSN t1  
                                                WHERE t1.A_C_ID = t2.A_C_ID FOR XML PATH('')
                                            ), 1 ,1, ''
                                            ) [TI_Symbol]

		                FROM BACKOFFICE.IAVMPSN t2
		                GROUP BY t2.A_C_ID,t2.Tran_Summ_Business_Date

	                )	p 
            ON p.A_C_ID = c.A_C_ID
                AND p.Tran_Summ_Business_Date = 
					(   SELECT 
                            Last_Maint_Date
                        FROM
                            BACKOFFICE.IAVMSM
                        WHERE
                            ti_alternate_id = '000009'
                    )
WHERE
   	    c.A_C_Open_Date IS NOT NULL
        AND c.A_C_Closing_Date IS NOT NULL
		AND c.A_C_Closing_Date &lt;= @RptDate
        AND (ISNULL(v.A_C_Summ_Equity, 0) != 0
            OR ISNULL(v.A_C_Summ_Mkt_Value, 0) != 0
            OR p.TI_Symbol IS NOT NULL
            )
ORDER BY r.branchCode, r.repName, c.A_C_Representative, c.A_C_ID;
</CommandText>
      </Query>
