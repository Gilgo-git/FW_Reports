  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>iAS_Portal</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ParmDate">
            <Value>=Parameters!ParmDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT  act.A_C_ID,act.Client_Short_Name,doc.Doc_Date,doc_type_2,doc.Doc_Desc, doc.Doc_Comments_1,doc.Doc_Comments_2,Doc_Comments_3, act.A_C_Sw_11 as buy_restriction,A_C_Sw_12 as sell_restriction
  FROM [iAS_Portal].[BACKOFFICE].[IAVMDOC] doc
	inner join [iAS_Portal].[BACKOFFICE].[IAVMACT] act
  on act.Client = doc.Client
    where Doc_Date &gt;=@ParmDate  and Doc_Date &lt; '2022-09-12'
	and (act.A_C_Sw_11='R' or act.A_C_Sw_12='R')
	and doc_type_2 in('31','22')
	order by   doc.Doc_Date ,act.A_C_ID</CommandText>
      </Query>
      <Fields>
        <Field Name="A_C_ID">
          <DataField>A_C_ID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Client_Short_Name">
