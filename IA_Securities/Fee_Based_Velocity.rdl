  <DataSources>
    <DataSource Name="iAS_Portal">
      <DataSourceReference>iAS_Portal</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>4eb1aa89-fbde-43da-82fd-865a305aa135</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>iAS_Portal</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Date">
            <Value>=Parameters!Date.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--DECLARE @Date as date = '2021-04-30'

DECLARE @YEND as date = (SELECT top 1 MAX(processDate) FROM [IAVM].[position] (nolock) 
							WHERE processDate &lt;= CONVERT(Date, CONVERT(nvarchar(4),YEAR(@Date)-1)+'-12-31'))


SELECT 
	case when [IAVMACT].ASC_3_Managed_Type in ('F','S','R') THEN 'Summit'
	when [IAVMACT].ASC_3_Managed_Type in ('L','X') THEN 'Diversiflex'
	when [IAVMACT].ASC_3_Managed_Type in ('M','Q') THEN 'Managed'
	when [IAVMACT].ASC_3_Managed_Type in ('V','N') THEN 'Vintage'
	when [IAVMACT].ASC_3_Managed_Type ='J' THEN 'Strategic Portfolio'
	END AS [Platform]
	,CASE WHEN LEFT(IAVMACT.A_C_ID,2) = '37' THEN 'IAS' WHEN LEFT(IAVMACT.A_C_ID,2) = '38' THEN 'HW' END AS [Broker]
	,rep.repName
	,rep.repId
	,SUM(b.valueCAD) AS PYE_AUA
	,SUM(position.valueCAD) as AUA
	
FROM [BACKOFFICE].[IAVMACT] (nolock)
	LEFT OUTER JOIN [IAVM].[rep] (nolock)
		ON [IAVMACT].A_C_Representative = rep.repId
	INNER JOIN [IAVM].[position] (nolock)
		ON [IAVMACT].A_C_ID = position.accountId
	LEFT OUTER JOIN [IAVM].[position] b (nolock)
		ON position.accountId = b.accountId
		AND b.processDate = @YEND
WHERE position.processDate = @Date
and ([IAVMACT].A_C_Closing_Date &gt;= @Date OR NULLIF([IAVMACT].A_C_Closing_Date,'') IS NULL)
AND [IAVMACT].ASC_3_Managed_Type in ('F','S','R','L','X','M','Q','V','N','J')
AND LEFT(IAVMACT.A_C_ID,2) IN ('37','38')
AND rep.repId IS NOT NULL
GROUP BY rep.repName
	,rep.repId
	,case when [IAVMACT].ASC_3_Managed_Type in ('F','S','R') THEN 'Summit'
	when [IAVMACT].ASC_3_Managed_Type in ('L','X') THEN 'Diversiflex'
	when [IAVMACT].ASC_3_Managed_Type in ('M','Q') THEN 'Managed'
	when [IAVMACT].ASC_3_Managed_Type in ('V','N') THEN 'Vintage'
	when [IAVMACT].ASC_3_Managed_Type ='J' THEN 'Strategic Portfolio'
	END
	,CASE WHEN LEFT(IAVMACT.A_C_ID,2) = '37' THEN 'IAS' WHEN LEFT(IAVMACT.A_C_ID,2) = '38' THEN 'HW' END</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
