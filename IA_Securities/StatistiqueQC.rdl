	<DataSources>
		<DataSource Name="iAS_Portal">
			<DataSourceReference>iAS_Portal</DataSourceReference>
			<rd:SecurityType>None</rd:SecurityType>
			<rd:DataSourceID>03bf3f2d-d423-40d9-b687-fd2c71bd9b48</rd:DataSourceID>
		</DataSource>
	</DataSources>
	<DataSets>
		<DataSet Name="statistiqueQC">
			<Query>
				<DataSourceName>iAS_Portal</DataSourceName>
				<CommandText>
					;WITH CTE_Core_Selection AS
					(
					SELECT --DISTINCT
					p.Tran_Summ_Business_Date AS processDate,
					r.repId AS R_repId,
					a.A_C_Representative,
					a.A_C_ID AS A_A_C_ID,
					p.A_C_ID AS P_A_C_ID,
					p.Tran_Summ_Business_Date,
					pc.typeCode,
					RTRIM(p.TI_Alternate_TI_Type) AS TI_Alternate_TI_Type,
					RTRIM(p.TI_Type) AS TI_Type,
					pc.classCode,
					p.TI_Alternate_TI_Class,
					a.A_C_Open_Date,
					a.Client_Residence_Region,
					p.Tran_Summ_Curr_Mkt_Value,
					r.branchCode,
					r.repId,
					a.A_C_Client AS clientId,
					a.A_C_ID AS accountId,
					a.A_C_Currency AS accountCurrency,
					a.A_C_Open_Date AS accountOpenDate,
					pc.productCode,
					CASE WHEN RTRIM(TI_Type) = 'CD' THEN 'COM'
					WHEN RTRIM(TI_Type) IN ('BD' , 'AB', 'MM', 'ST') THEN 'FIX'
					WHEN RTRIM(TI_Type) = 'EQ' THEN 'EQT'
					WHEN RTRIM(TI_Type) IN ('OD') THEN 'GIC'
					WHEN RTRIM(TI_Type) IN ('MF') THEN 'MUT'
					WHEN RTRIM(TI_Type) IN ('CR' , 'IX') THEN 'OPT'
					ELSE 'EQT'
					END AS Derived_productCode
					--	Mapping LEN(TI_Type) from 2 to 3 as productCode
					FROM BACKOFFICE.IAVMACT a
					LEFT OUTER JOIN IAVM.rep r
					ON r.repId = a.A_C_Representative
					INNER JOIN BACKOFFICE.IAVMPSN p
					ON a.A_C_ID = p.A_C_ID
					AND p.Tran_Summ_Business_Date IN
					(
					SELECT DISTINCT Last_Maint_Date
					FROM BACKOFFICE.IAVMSM
					WHERE ti_alternate_id = '000009'
					)
					LEFT OUTER JOIN	IAVM.product pc
					ON pc.typeCode = RTRIM(p.TI_Alternate_TI_Type)
					AND pc.classCode = p.TI_Alternate_TI_Class
					WHERE a.A_C_Open_Date IS NOT Null
					AND a.Client_Residence_Region = 'QC'
					),
					CTE_Core_Sum AS
					(
					SELECT processDate, R_repId, A_C_Representative, A_A_C_ID, P_A_C_ID, Tran_Summ_Business_Date,
					typeCode,
					TI_Alternate_TI_Type,
					TI_Type, classCode,
					TI_Alternate_TI_Class,
					A_C_Open_Date,
					Client_Residence_Region,
					Tran_Summ_Curr_Mkt_Value,
					branchCode,
					repId,
					clientId,
					accountId,
					accountCurrency,
					accountOpenDate,
					productCode,
					Derived_productCode,
					ROW_NUMBER()
					OVER(PARTITION BY Tran_Summ_Business_Date, branchCode , repId , clientId, accountId, Derived_productCode
					ORDER BY Tran_Summ_Business_Date, branchCode , repId , clientId, accountId, Derived_productCode)
					AS DeDupCounts,
					SUM(Tran_Summ_Curr_Mkt_Value)
					OVER(PARTITION BY Tran_Summ_Business_Date, branchCode , repId , clientId, accountId, Derived_productCode
					ORDER BY Tran_Summ_Business_Date, branchCode , repId , clientId, accountId, Derived_productCode)
					AS [value]
					FROM CTE_Core_Selection
					)
					SELECT processDate,
					branchCode,
					repId,
					clientId,
					accountId,
					accountCurrency,
					accountOpenDate,
					Derived_productCode AS productCode,
					[value]
					FROM CTE_Core_Sum
					WHERE DeDupCounts = 1

					UNION

					SELECT DISTINCT
					v.A_C_SUMM_BALANCE_BUS_DATE AS processDate,
					r.branchCode,
					r.repId,
					a.A_C_Client AS clientId,
					a.A_C_ID AS accountId,
					a.A_C_Currency AS accountCurrency,
					a.A_C_Open_Date AS accountOpenDate,
					'CASH' AS productCode,
					(v.A_C_Summ_TD_Net_Amt * - 1) AS value
					FROM BACKOFFICE.IAVMACT a
					INNER JOIN IAVM.rep r
					ON r.repId = a.A_C_Representative
					INNER JOIN BACKOFFICE.IAVMVAL v ON a.A_C_ID = v.A_C_ID
					AND v.A_C_SUMM_BALANCE_BUS_DATE IN
					(
					SELECT DISTINCT Last_Maint_Date
					FROM BACKOFFICE.IAVMSM
					WHERE ti_alternate_id = '000009'
					)
					WHERE a.A_C_Open_Date IS NOT Null
					AND a.Client_Residence_Region = 'QC'
				</CommandText>
			</Query>
