  <DataSets>
    <DataSet Name="OffshoreAccounts">
      <Query>
        <DataSourceName>iAS_Portal</DataSourceName>
        <CommandText>SELECT DISTINCT
	CASE WHEN SUBSTRING(A.A_C_ID, 1, 2) = '38' 
		 THEN 'HW' 
		 ELSE 'IAS' 
		 END AS Broker,
	A.A_C_Representative AS [RR_Code],
	R.repName AS [Rep_Name],
	A.Client AS [ClientID],
	A.A_C_ID AS [AccountID],
	A.A_C_Name AS [Account_Name],
	A.Account_Rap_Code AS [RapCode],
	CASE WHEN A.Account_Rap_Code IN ('A', 'B') THEN 'Cash'
		 WHEN A.Account_Rap_Code IN ('C', 'D') THEN 'COD'
		 WHEN A.Account_Rap_Code IN ('E', 'F') THEN 'Margin'
		 WHEN A.Account_Rap_Code IN ('G', 'H') THEN 'Margin Short'
		 WHEN A.Account_Rap_Code IN ('I') THEN 'IPP'
		 WHEN A.Account_Rap_Code IN ('K', 'J') THEN 'Income Account Generation'
		 WHEN A.Account_Rap_Code IN ('N', '9') THEN 'LIRA'
		 WHEN A.Account_Rap_Code IN ('0') THEN 'RDSP'
		 WHEN A.Account_Rap_Code IN ('O') THEN 'Off Book Mutual Fund Account'
		 WHEN A.Account_Rap_Code IN ('P') THEN 'LIF'
		 WHEN A.Account_Rap_Code IN ('R', '8') THEN 'Spousal RRSP'
		 WHEN A.Account_Rap_Code IN ('Q') THEN 'PSSP'
		 WHEN A.Account_Rap_Code IN ('S', '7') THEN 'RRSP'
		 WHEN A.Account_Rap_Code IN ('T') THEN 'RIF'
		 WHEN A.Account_Rap_Code IN ('U', 'V') THEN 'Hedge'
		 WHEN A.Account_Rap_Code IN ('W', '6') THEN 'TFSA'
		 WHEN A.Account_Rap_Code IN ('X') THEN 'Pledge/Collateral Account'
		 WHEN A.Account_Rap_Code IN ('Y') THEN 'Spousal RIF'
		 ELSE 'RESP' 
	END AS [AccountType],
	A.ASC_3_Managed_Type AS [ASC_Code],
	A.A_C_Open_Date AS [Open_Date],
	A.Client_Residence AS [ClientResidence],
	C.[Country],
	A.Client_Residence_Region AS [ClientResidenceRegion],
	A.CITIZENSHIP,
	-1 * V.A_C_Summ_Equity AS [AccountMktValue]
FROM 
	BACKOFFICE.IAVMACT AS A
	INNER JOIN IAVM.rep AS R 
	ON A.A_C_Representative = R.repId
	LEFT OUTER JOIN BACKOFFICE.IAVMVAL AS V 
	ON A.A_C_Representative = V.A_C_Representative 
	AND A.A_C_ID = V.A_C_ID
	LEFT OUTER JOIN [IAVM].[countryInfo] AS C
	ON RTRIM(A.[Client_Residence]) = C.[Abbreviation]
WHERE  
	LEN(A.A_C_ID) &gt; 2 
	AND A.A_C_CLOSING_DATE IS Null
	AND A.Client_Residence != 'CAN'
				</CommandText>
      </Query>
