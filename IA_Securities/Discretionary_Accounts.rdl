  <DataSources>
    <DataSource Name="iAS_Portal">
      <DataSourceReference>iAS_Portal</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>253da58d-7667-4ada-af16-5adc907c2ce4</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>iAS_Portal</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@processDate">
            <Value>=Parameters!processDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
Declare @processDate1 as Date =@processDate
--select @processDate1 = Max(processDate) from [IAVM].[position]
--select @processDate

SELECT processDate,p.repID,[repName]--,accountID
,SUM(ValueCad) Market_Value FROM [IAVM].[position] p
LEFT JOIN  [IAVM].[rep] r ON p.repId=r.repId
WHERE processDate=@processDate1
AND accountID IN (

 SELECT [A_C_ID]
  FROM [iAS_Portal].[BACKOFFICE].[IAVMACT]
  WHERE [ASC_3_Managed_Type]&lt;&gt;'0' AND [A_C_Closing_Date] IS NULL
  )
  GROUP BY processDate,p.repID,[repName]
  ORDER BY  processDate,[repName]</CommandText>
      </Query>
      <Fields>
        <Field Name="processDate">
          <DataField>processDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="repID">
          <DataField>repID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="repName">
          <DataField>repName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Market_Value">
          <DataField>Market_Value</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="processDate">
      <Query>
        <DataSourceName>iAS_Portal</DataSourceName>
        <CommandText>select  Max(processDate) processDate from [IAVM].[position]</CommandText>
      </Query>
      <Fields>
        <Field Name="processDate">
          <DataField>processDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
