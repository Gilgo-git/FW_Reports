				</Field>
				<Field Name="productCode">
					<DataField>productCode</DataField>
					<rd:TypeName>System.String</rd:TypeName>
				</Field>
				<Field Name="value">
					<DataField>value</DataField>
					<rd:TypeName>System.Decimal</rd:TypeName>
				</Field>
			</Fields>
		</DataSet>
	</DataSets>
