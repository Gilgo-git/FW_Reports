        <CommandText>  SELECT 
MAX([Mkt_Price_Date]) AS MK_DATE	
, CONVERT(Date, DATEADD(MONTH, DATEDIFF(MONTH, 0, GETDATE())-1, 0)) AS [Start] 
, CONVERT(Date, DATEADD(MONTH, DATEDIFF(MONTH, -1, GETDATE())-1, -1)) AS [End] 

