  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>CERTS2</DataSourceName>
        <CommandText>SELECT DISTINCT
	E.FIRST_NM                                           [FirstName]
	,E.LAST_NM                                            [LastName]
	,Lib.empNameLFN (E.FIRST_NM, E.MIDDLE_NM, E.LAST_NM)  [FullName]
	,EMAIL
	,isnull (E.isEmployee, 'N')                           [IsEmployee]
	,E.ACTIVE_YN                                          [IsActive]
	,BUP.BUSINESS_UNIT_POSITION_NM                        [PrimaryRole]           -- NULL-able
	,title
	,case when BUP.rolCtg = 'ADV' then 'Y' else 'N' end   [IsPrimaryRoleAdvisor]  -- NULL-able
	,Lib.empNameLFN (E_CBM.FIRST_NM, E_CBM.MIDDLE_NM, E_CBM.LAST_NM)     [CBM]
		,Lib.empNameLFN (E_RVP.FIRST_NM, E_RVP.MIDDLE_NM, E_RVP.LAST_NM)     [RVP]
		,DirectLine
	,isnull (BU.BUSINESS_UNIT_UID   ,'')                                 [BranchNRD]
	,isnull (BU.STREET_ADDRESS1_TX  ,'')                                 [Address1]
	,isnull (BU.STREET_ADDRESS2_TX  ,'')                                 [Address2]
	,isnull (BU.STREET_CITY_NM      ,'')                                 [City]
	,isnull (BU.[STREET_STATE_ID]  ,'')                                 [Province]
	,isnull (BU.STREET_ZIP_ID       ,'')                                 [PostalCode]				
	, E.[LANGUAGE_ID] as language
	
into #t1

FROM  EMPLOYEE E

LEFT JOIN
(
	select distinct e.EMPLOYEE_ID,phone_number_tx as DirectLine from  EMPLOYEE_PHONE E
	INNER JOIN     PHONE_TYPE  PT
	on  PT.PHONE_TYPE_ID = E.PHONE_TYPE_ID
	LEFT JOIN EmpPrimary  P
	on  P.eid = E.EMPLOYEE_ID
	where e.phone_type_Id in ('1') ---Direct Line
) tel1
	on e.EMPLOYEE_ID=tel1.EMPLOYEE_ID

LEFT JOIN
(
	select distinct e.EMPLOYEE_ID,
	phone_number_tx  as EMAIL from  EMPLOYEE_PHONE       E
	INNER JOIN     PHONE_TYPE           PT
	on  PT.PHONE_TYPE_ID = E.PHONE_TYPE_ID
	and PT.ctg           = 'E'                -- 'E'mail
	LEFT JOIN EmpPrimary               P
	on  P.eid = E.EMPLOYEE_ID
	and P.typ = 'E'                           -- 'E'mail
) EMAIL
	on EMAIL.EMPLOYEE_ID=E.EMPLOYEE_ID

-- primary role
-- ------------
LEFT JOIN     
	ViewPrimaryRole VPR
	on VPR.eid = E.EMPLOYEE_ID

LEFT JOIN     
	BUSINESS_UNIT_POSITION BUP
	on BUP.BUSINESS_UNIT_POSITION_ID = VPR.rolId

LEFT JOIN   
	EmpPrimary  EPA
	ON   EPA.eid = E.EMPLOYEE_ID
	and EPA.typ = 'A'       -- Address
	and EPA.src = 'B'       -- Branch address

LEFT JOIN   
	BUSINESS_UNIT                BU
	on  BU.BUSINESS_UNIT_ID = EPA.ref
	and BU.CLOSE_DT IS NULL    -- active branches
	and BU.ACTIVE_YN = 'Y'

-- Relationship-based supervision
-- ------------------------------
LEFT JOIN     
	EmpPrimary EP1
	on  EP1.eid = E.EMPLOYEE_ID
	and EP1.typ = 'S'                         -- Supervisor
	and EP1.src = 'R'                         -- Relationship

LEFT JOIN     
	EmpRelationship ER1
	on ER1.ck = EP1.ref

-- Branch-based supervision
-- ------------------------
LEFT JOIN 
	EmpPrimary  EP2
	on  EP2.eid = E.EMPLOYEE_ID
	and EP2.typ = 'S'                         -- Supervisor
	and EP2.src = 'B'                         --  Branch

LEFT JOIN     
	BUSINESS_UNIT           BU2
	on BU2.BUSINESS_UNIT_ID = EP2.ref

-- advisor title
-- -------------
LEFT JOIN
	[ViewTitle] title
	on title.eid=e.EMPLOYEE_ID

-- advisor roles
-- -------------
LEFT JOIN
(
	SELECT   eid, 'Y' [isAdvisor]
	FROM     ViewRolesAtBranches     VRB
	JOIN     BUSINESS_UNIT_POSITION  BUP
	on     BUP.BUSINESS_UNIT_POSITION_ID = VRB.rolId
	WHERE    BUP.rolCtg = 'ADV'
	GROUP BY eid
) ROL
	on ROL.eid = E.EMPLOYEE_ID

-- RVP
-- ---
LEFT JOIN   
(
	SELECT
	REL_UP.eid1
	,MAX (REL_UP.eid2)  [eid2]                             -- TODO: there should ONLY be 1x upwards relationship with RVP-s
	FROM EmpRelationship  REL_UP
	INNER JOIN EMPLOYEE_ROLE          ER2
	on  ER2.EMPLOYEE_ID               = REL_UP.eid2
	and ER2.BUSINESS_UNIT_POSITION_ID = '515'                -- RVP / TODO: hard-coded
	-- assert: and ER2.BUSINESS_UNIT_LOCATION_ID === NULL
	and ER2.END_DT IS NULL                                   -- active roles   
	WHERE REL_UP.ACTIVE_YN = 'Y'
	GROUP BY REL_UP.eid1
) REL_UP
	on REL_UP.eid1 = E.EMPLOYEE_ID

LEFT JOIN 
	EMPLOYEE E_RVP
	on  E_RVP.EMPLOYEE_ID = REL_UP.eid2
	and E_RVP.ACTIVE_YN   = 'Y'

-- CBM
-- ---
LEFT JOIN
(
	SELECT   ER.EMPLOYEE_ID
	,ER.BUSINESS_UNIT_LOCATION_ID
	FROM     EMPLOYEE_ROLE           ER
	JOIN     BUSINESS_UNIT_POSITION  BUP
	on  BUP.BUSINESS_UNIT_POSITION_ID = ER.BUSINESS_UNIT_POSITION_ID
	and BUP.rolCtg                    = 'CBM'
	WHERE    ER.END_DT IS NULL
) ER_BRM
on  ER_BRM.BUSINESS_UNIT_LOCATION_ID = BU.BUSINESS_UNIT_ID     -- !! there should be &lt;=1! such records

LEFT JOIN 
	EMPLOYEE E_CBM
	on  E_CBM.EMPLOYEE_ID = ER_BRM.EMPLOYEE_ID
	and E_CBM.ACTIVE_YN = 'Y'

WHERE E.company = 'IAS'  -- and  (BUP.rolCtg = 'ADV') -- Advisors only
	and E.ACTIVE_YN ='Y'

ORDER BY E.LAST_NM

---NEXT QUERY
SELECT DISTINCT
	LastName,	FirstName,	FullName,	IsEmployee,	PrimaryRole	,
	isnull(title,'') as Title, IsPrimaryRoleAdvisor,	 
	STUFF((SELECT DISTINCT ',' + E2.email
	FROM #t1 E2
	WHERE T1.FullName= E2.FullName  
	FOR XML PATH (''), TYPE, ROOT).value('root[1]', 'nvarchar(max)'), 1, 1, '') AS Emails,
	Address1,	Address2,	City,	Province	,PostalCode,	
	STUFF((SELECT DISTINCT ',' + E2.DirectLine
	FROM #t1 E2
	WHERE T1.FullName= E2.FullName
	FOR XML PATH (''), TYPE, ROOT).value('root[1]', 'nvarchar(max)'), 1, 1, '') AS DirectLines,
	CBM,	RVP,	Language,	IsActive
   
FROM #t1 t1

ORDER BY LastName</CommandText>
