      </Fields>
    </DataSet>
    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>iAS_Portal</DataSourceName>
        <CommandText>DECLARE @StartDate DATE, @EndDate DATE
set @EndDate = DATEADD (dd, -1, DATEADD(dd, DATEDIFF(dd, 0, GETDATE()) , 0));
set @StartDate=DATEADD (dd, -30, DATEADD(dd, DATEDIFF(dd, 0, GETDATE()) , 0));

WITH ListDates(AllDates) AS
(    SELECT @StartDate AS DATE
    UNION ALL
    SELECT DATEADD(DAY,1,AllDates)
    FROM ListDates 
    WHERE AllDates &lt; @EndDate)

SELECT CAST(CONVERT(Date, DATEADD (dd, -1,AllDates),101)as VARCHAR) AS AllDates
FROM ListDates
ORDER BY AllDates DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="AllDates">
          <DataField>AllDates</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
