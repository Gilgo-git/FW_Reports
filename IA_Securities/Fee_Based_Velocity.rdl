    <DataSet Name="Default_Date">
      <Query>
        <DataSourceName>iAS_Portal</DataSourceName>
        <CommandText>  SELECT 
	MAX(processDate) AS [MonthEnd] 

  FROM [IAVM].[position] (nolock)
  WHERE processDate &lt;= CONVERT(Date, DATEADD(MONTH, DATEDIFF(MONTH, -1, GETDATE())-1, -1)) </CommandText>
      </Query>
